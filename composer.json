{
    "name": "laravel-api/youtube",
    "description": "This is my package youtube",
    "homepage": "https://github.com/laravapi/youtube",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Sch√∂ps",
            "email": "sebastian.schoeps@nanuc.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "alaouy/youtube": "^2.2"
    },
    "require-dev": {
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "LaravelApi\\YouTube\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelApi\\YouTube\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
